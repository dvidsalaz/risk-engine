    "#Array Fundamentals\n",
    "\n",
    "#An array is a structure for storing and retrieving data\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "a = np.array([1, 2, 3, 4, 5, 6])\n",
    "print(a)\n",
    "\n",
    "#Accessing an Array through Index\n",
    "\n",
    "a[0] = 10\n",
    "\n",
    "print(a)\n",
    "\n",
    "#Slice notation can be used\n",
    "a[:3]\n",
    "\n"
   ]
  }
